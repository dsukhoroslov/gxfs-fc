. 

gax-participant:salesVolume
	a		 owl:DatatypeProperty ;
	rdfs:label		 "sales volume"@en ;
	rdfs:domain		 gax-participant:TrustedCloudProvider ;
	rdfs:range		 xsd:string ;
	rdfs:comment		 "Sales volume of the organisation." ;
. 

gax-participant:category
	a		 owl:DatatypeProperty ;
	rdfs:label		 "category"@en ;
	rdfs:domain		 gax-participant:TrustedCloudProvider ;
	rdfs:range		 xsd:enum ;
	rdfs:comment		 "Specific category of the organisation." ;
. 

gax-participant:countPublicCloudServices
	a		 owl:DatatypeProperty ;
	rdfs:label		 "count public cloud services"@en ;
	rdfs:domain		 gax-participant:TrustedCloudProvider ;
	rdfs:range		 xsd:string ;
