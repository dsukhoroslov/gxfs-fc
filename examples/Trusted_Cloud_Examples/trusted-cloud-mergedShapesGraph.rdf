                "Information about technical and organizational measures for GDPR."@en ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "gdpr measures" ;
            sh:node gax-validation:GdprMeasuresShape ;
            sh:order 1 ;
            sh:path trusted-cloud:gdprMeasures ] ;
