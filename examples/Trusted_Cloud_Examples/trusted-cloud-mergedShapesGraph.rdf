gax-validation:ContractLawShape a sh:NodeShape ;
    sh:property [ skos:example "'Berlin', 'Paris', 'Rome'" ;
            sh:datatype xsd:string ;
            sh:description "Das EU -Vertragsrecht führt zu einem Ort der Gerichtsbarkeit in der EU.Geben Sie den richtigen Ort wie im Vertrag geschrieben."@de,
                "EU contract law results in a place of jurisdiction in the EU. Give the right place as written in the contract."@en ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "place of jurisdiction" ;
            sh:order 2 ;
            sh:path trusted-cloud:placeOfJurisdiction ],
        [ skos:example "'german contract law', 'EU contract law'" ;
            sh:datatype xsd:string ;
            sh:description "Wenn das EU -Vertragsgesetz ein korrektes Vertragsgesetz ist, muss in einem FreeText -Bereich geschrieben werden"@de,
                "If EU contract law, a correct contract law has additinally to be written in a freetext field"@en ;
            sh:in ( "german contract law" "EU contract law" ) ;
            sh:maxCount 1 ;
            sh:name "applicable law" ;
            sh:order 1 ;
            sh:path trusted-cloud:applicableLaw ] ;
    sh:targetClass trusted-cloud:ContractLaw .

gax-validation:ContractShape a sh:NodeShape ;
    sh:property [ skos:example "'(a structured object of type ExitSupport.'" ;
            sh:description "Informationen zu Regeln für den Austrittsunterstützung (Exit-Management), insbesondere das angepasste Feedback für Kundendaten."@de,
                "Information about rules for exit support (Exit-Management), especially the adjusted feedback for customer data."@en ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "exit support" ;
            sh:node gax-validation:ExitSupportShape ;
            sh:order 10 ;
            sh:path trusted-cloud:exitSupport ],
        [ skos:example "'(a structured object of type UsageRights.'" ;
            sh:description "Details zu Nutzungsrechten und Zuständigkeitsbereich."@de,
                "Details about rights of usage and area of jurisdiction."@en ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "rigths of usage" ;
            sh:node gax-validation:UsageRightsShape ;
            sh:order 2 ;
            sh:path trusted-cloud:rigthsOfUsage ],
        [ skos:example "'(a structured object of type SubcontractorTransparency.'" ;
            sh:description "Nennen Sie alle Concerd -Subunternehmer und -vereinbarungen für die Änderung während der Dauer der Dienstleistung sowie die Verpflichtung zu Datenschutzinformationen."@de,
                "Name all concerd subcontractors and agreements for change during duration of service provision as well as commitment on data privacy information."@en ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "transparancy with subcontractors" ;
            sh:node gax-validation:SubcontractorTransparencyShape ;
            sh:order 3 ;
            sh:path trusted-cloud:transparancyWithSubcontractors ],
        [ skos:example "'(a structured object of type SubcontractorTransparency.'" ;
            sh:description "Einzelheiten zu Regeln, die das Datenfeedback jederzeit garantieren."@de,
                "Details about rules which guarantee data feedback at any time."@en ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "interruption rules" ;
            sh:node gax-validation:ServiceInterruptionRulesShape ;
            sh:order 4 ;
            sh:path trusted-cloud:interruptionRules ],
        [ skos:example "'(a structured object of type ServiceLevelAgreement.'" ;
            sh:description "Informationen von Service Level Agreements (SLA) und Regeln für die SLA-Gewalt."@de,
                "Information of Service Level Agreements (SLA) and rules for SLA-violation."@en ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "sla" ;
            sh:node gax-validation:ServiceLevelAgreementShape ;
            sh:order 5 ;
            sh:path trusted-cloud:sla ],
        [ skos:example "'(a structured object of type PricingModel.'" ;
            sh:description "Informationen zu Gebrauchtpreismodell, Vertragsdauer und Kündigung der Stornierung."@de,
                "Information about used price model, contract duration and notice of cancellation."@en ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "pricing model" ;
            sh:node gax-validation:PricingModelShape ;
            sh:order 6 ;
            sh:path trusted-cloud:pricingModel ],
        [ skos:example "'(a structured object of type ChangeManagement.'" ;
            sh:description "Informationen zu vertraglichen Regeln für den Umgang mit Änderungen."@de,
                "Information of contractual rules for handling changes."@en ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "change management" ;
            sh:node gax-validation:ChangeManagementShape ;
            sh:order 7 ;
            sh:path trusted-cloud:changeManagement ],
        [ skos:example "'true', 'false'" ;
            sh:datatype xsd:boolean ;
            sh:description "Sind alle Verpflichtungen zur Zusammenarbeit oder Bestimmungen im Vertrag definiert?"@de,
                "Are all obligations to cooperate or provisions defined in the contract?"@en ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "obligation to cooperate in contract" ;
            sh:order 8 ;
            sh:path trusted-cloud:obligationToCooperateInContract ],
        [ skos:example "'true', 'false'" ;
            sh:datatype xsd:boolean ;
            sh:description "Ist der Kunde während der Nutzung dieses Dienstes von allen Ansprüchen von Dritten befreit?"@de,
                "Is the customer exempt from all claims of third parties while using this service?"@en ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "customer exemption from claims of third parties" ;
            sh:order 9 ;
            sh:path trusted-cloud:customerExemptionFromClaimsOfThirdParties ],
        [ skos:example "'(a structured object of type ContractTransparency.'" ;
            sh:description "Informationen zur allgemeinen Vertragstransparenz."@de,
                "Information about general contract transparency."@en ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "contract transparency" ;
            sh:node gax-validation:ContractTransparencyShape ;
            sh:order 1 ;
            sh:path trusted-cloud:contractTransparency ] ;
    sh:targetClass trusted-cloud:Contract .

gax-validation:DataCentreShape a sh:NodeShape ;
