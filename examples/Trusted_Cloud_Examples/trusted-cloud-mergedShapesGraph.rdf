gax-validation:DataProtectionShape a sh:NodeShape ;
    sh:property [ skos:example "'(a structured object of type GdprCompliance)'" ;
            sh:description "Informationen zur Einhaltung der europäischen allgemeinen Datenschutzverordnung (DSGVO)"@de,
                "Information of compliance of European General Data Protection Regulation (GDPR)"@en ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "gdpr compliance" ;
            sh:node gax-validation:GdprComplianceShape ;
            sh:order 2 ;
            sh:path trusted-cloud:gdprCompliance ],
        [ skos:example "'a structured object of type DataRetention)'" ;
            sh:description "Informationen zur Begrenzung von Kundendaten in bestimmte Regionen"@de,
                "Information about limitation of customer data to specific regions"@en ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "data retention" ;
            sh:node gax-validation:DataRetentionShape ;
            sh:order 3 ;
            sh:path trusted-cloud:dataRetention ],
        [ skos:example "'/a structured object of type DataSubjectRights)'" ;
            sh:description "Einzelheiten zur Implementierung der betroffenen Rechte gemäß ART.12-23 DSGVO?"@de,
                "Details on the implementation of the data subject rights in accordance with Art. 12-23 GDPR?"@en ;
            sh:maxCount 1 ;
