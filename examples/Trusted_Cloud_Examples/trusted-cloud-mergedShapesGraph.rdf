@prefix gax-core: <https://w3id.org/gaia-x/core#> .
@prefix gax-validation: <https://w3id.org/gaia-x/validation#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix trusted-cloud: <http://w3id.org/gaia-x/trusted-cloud#> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

gax-validation:AuditabilityProfileShape a sh:NodeShape ;
    sh:property [ skos:example "'data protection audits'" ;
            sh:datatype xsd:string ;
            sh:description "Beschreibung der bereits durchgeführten Audits."@de,
                "Description of audits which were already performed."@en ;
            sh:minCount 1 ;
            sh:name "audits" ;
            sh:order 2 ;
            sh:path trusted-cloud:audits ],
        [ skos:example "'by the user', 'by a mandated auditor'" ;
            sh:datatype xsd:string ;
            sh:description "Können Audits zu Prozessen und organisatorischen Verfahren im Zusammenhang mit Datenschutz und Sicherheit durchgeführt werden?"@de,
                "Can audits on processes and organizational procedures related to data protection and security be conducted?"@en ;
            sh:in ( "by the user" "by a mandated auditor" ) ;
            sh:maxCount 1 ;
            sh:name "auditor" ;
            sh:order 3 ;
            sh:path trusted-cloud:auditor ],
        [ skos:example "'yes', 'no'" ;
            sh:datatype xsd:boolean ;
            sh:description "Ist es dem Benutzer möglich, ein Audit anzufordern?"@de,
                "Is it possible for the user to request an audit?"@en ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "auditability" ;
            sh:order 1 ;
            sh:path trusted-cloud:auditability ] ;
    sh:targetClass trusted-cloud:AuditabilityProfile .

gax-validation:CertificateShape a sh:NodeShape ;
    sh:property [ skos:example "'TÜV Süd'" ;
            sh:description "Zertifikatbehörde für diesen Dienst oder sein Zertifikat"@de,
                "Certificate authority for this service or its certificate"@en ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "certificate authority" ;
            sh:node gax-validation:CertificationBodyShape ;
            sh:order 3 ;
            sh:path trusted-cloud:certificateAuthority ],
        [ skos:example "'2122-12-21'" ;
            sh:datatype xsd:date ;
            sh:description "Datum, an dem das Zertifikat abläuft"@de,
                "Date on which the certificate expires"@en ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "expiration date" ;
            sh:order 4 ;
            sh:path trusted-cloud:expirationDate ],
        [ skos:example "'true', 'false'" ;
            sh:datatype xsd:boolean ;
            sh:description "Wird das Zertifikat regelmäßig geprüft?"@de,
                "Is the certificate regularly audited?"@en ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "regular audited" ;
            sh:order 5 ;
            sh:path trusted-cloud:regularAudited ],
        [ skos:example "'BSI IT-Grundschutz', 'ISO 27001', 'Trust in Cloud'" ;
            sh:datatype xsd:string ;
            sh:description "Art der Zertifikate"@de,
                "Type of the certificates"@en ;
            sh:in ( "BSI IT-Grundschutz" "CSA Star" "EuroCloud Star Audit" "EuroPrise" "FedRAMP" "ISAE 3402/SSAE 16 Typ II" "ISO 27001" "ISO 27018" "Trust in Cloud" "Trusted Cloud - TÜV" "TRUSTed Cloud Privacy" "TÜV Cloud Security" "TCDP" "others" ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "type" ;
            sh:order 2 ;
            sh:path trusted-cloud:type ],
        [ skos:example "'ISO XYZ'" ;
            sh:datatype xsd:string ;
            sh:description "Name des Zertifikats"@de,
                "Name of certificate"@en ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "name" ;
            sh:order 1 ;
            sh:path trusted-cloud:name ] ;
    sh:targetClass trusted-cloud:Certificate .

gax-validation:CompanyProfileProviderShapeTC a sh:NodeShape ;
    sh:property [ skos:example "'<1 year', '1-5 year', '>5 year'" ;
            sh:datatype xsd:string ;
            sh:description "Erfahrung / Kenntnis der Organisation im Cloud -Service -Geschäft."@de,
                "Experience / knowledge of the organization in the cloud service business."@en ;
            sh:in ( "<1 year" "1-5 year" ">5 year" ) ;
            sh:maxCount 1 ;
            sh:name "experience cloud services" ;
            sh:order 6 ;
            sh:path trusted-cloud:experienceCloudServices ],
        [ skos:example "'50'" ;
            sh:datatype xsd:integer ;
            sh:description "Mitarbeiterzahl der Organisation im Cloud -Servicebereich."@de,
                "Employee count of the organization in the cloud service area."@en ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:minInclusive 1.0 ;
            sh:name "employee count for cloud services" ;
            sh:order 2 ;
            sh:path trusted-cloud:employeeCountForCloudServices ],
        [ skos:example "'50.000.000'" ;
            sh:datatype xsd:integer ;
            sh:description "Verkaufsvolumen der Organisation."@de,
                "Sales volume of the organization."@en ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:minInclusive 1.0 ;
            sh:name "sales volume" ;
            sh:order 3 ;
            sh:path trusted-cloud:salesVolume ],
        [ skos:example "'Reseller', 'Internet Service Provider', 'Cloud Service Provider', 'Others'" ;
            sh:datatype xsd:string ;
            sh:description "Spezifische Kategorie der Organisation."@de,
                "Specific category of the organization."@en ;
            sh:in ( "Reseller" "Internet Service Provider" "Cloud Service Provider" "Others" ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "category" ;
            sh:order 4 ;
            sh:path trusted-cloud:category ],
        [ skos:example "'5', '10'" ;
            sh:datatype xsd:integer ;
            sh:description "Öffentlich angebotene Cloud -Dienste."@de,
                "Publicly offered cloud services."@en ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:minInclusive 1.0 ;
            sh:name "number of public cloud services" ;
            sh:order 5 ;
            sh:path trusted-cloud:numberOfPublicCloudServices ],
        [ skos:example "'50'" ;
            sh:datatype xsd:integer ;
            sh:description "Mitarbeiterzahl der Organisation."@de,
                "Employee count of the organization."@en ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:minInclusive 1.0 ;
            sh:name "employee count" ;
            sh:order 1 ;
            sh:path trusted-cloud:employeeCount ] ;
    sh:targetClass trusted-cloud:CompanyProfileProvider .

